{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:ListAllMyBuckets",
                "s3:GetBucketLocation"
            ],
            "Resource": [
            "arn:aws:s3:::gehc-services-conmas-prod"
            ]
        },
        {
            "Sid": "ReadWriteObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
            "arn:aws:s3:::gehc-services-conmas-prod/*"
            ]
        }
    ]
}
