{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:ListBucketMultipartUploads"
            ],
            "Resource": [
                "arn:aws:s3:::gehc-ctt-artifactory"
            ]
        },
        {
            "Sid": "ReadWriteObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:AbortMultipartUpload",
                "s3:ListMultipartUploadParts"
            ],
            "Resource": [
                "arn:aws:s3:::gehc-ctt-artifactory/*"
            ]
        }
    ]
}
