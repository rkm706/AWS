{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
            "arn:aws:s3:::gels-digital-document-delivery"
            ]
        },
        {
            "Sid": "ReadWriteObjectsinBucket",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject"
            ],
            "Resource": [
            "arn:aws:s3:::gels-digital-document-delivery/*"
            ]
        }
    ]
}
